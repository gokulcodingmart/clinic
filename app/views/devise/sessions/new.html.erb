<h1 align="center">Log in</h1>
<br>
<div class="login" align="center">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field ">
   
    <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "Email", class: "search-input-login" %>
  </div>

  <div class="field" >
   
    <%= f.password_field :password, autocomplete: "off", placeholder: "Password", class: "search-input-login" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field" >
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>&nbsp&nbsp
      <%= link_to "Forgot your password?", new_password_path(resource_name) %><br />
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in", class: "search-input-login btn" %>
  </div>
<% end %>
</div>


